{
  "expo": {
    "name": "ColorFashionWheel",
    "slug": "fashion-color-wheel-backend",
    "scheme": "colorwheel",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B0B0C"
    },
    "assetBundlePatterns": ["**/*"],
    "extra": {
      "eas": { "projectId": "bdda9f6e-8e60-4d2f-882a-e1c37085af9b" }
    },
    "ios": {
      "bundleIdentifier": "com.fashioncolorwheel.app",
      "buildNumber": "50",
      "supportsTablet": false,
      "infoPlist": {
        "NSCameraUsageDescription": "Allow camera access to sample colors from photos.",
        "NSPhotoLibraryUsageDescription": "Allow photo library access to import images.",
        "NSPhotoLibraryAddUsageDescription": "Allow saving palettes/images to your library."
      }
    },
    "android": {
      "package": "com.fashioncolorwheel.app",
      "versionCode": 50,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_MEDIA_IMAGES"
      ]
    },
    "plugins": [
      "expo-image-picker",
      ["expo-build-properties", { "ios": { "deploymentTarget": "15.1" } }]
    ],
    "runtimeVersion": { "policy": "sdkVersion" }
  }
}