{
  "cli": {
    "version": ">= 16.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "image": "macos-sequoia-15.6-xcode-26.0"
      },
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://colorwheelapp-production.up.railway.app"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false,
        "buildConfiguration": "Release",
        "image": "macos-sequoia-15.6-xcode-26.0"
      },
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://colorwheelapp-production.up.railway.app"
      }
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "image": "macos-sequoia-15.6-xcode-26.0",
        "buildConfiguration": "Release"
      },
      "env": {
        "NODE_ENV": "production",
        "EXPO_PUBLIC_API_BASE_URL": "https://colorwheelapp-production.up.railway.app"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "JnVincentt@gmail.com",
        "ascAppId": "6749773346",
        "appleTeamId": "B8QUH58284"
      },
      "android": {
        "serviceAccountKeyPath": "./google-service-account.json",
        "track": "production"
      }
    }
  }
}
